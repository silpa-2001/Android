package com.example.database;

public class Student {
    private int id;
    private String name;
    private String course;
    private int sem;

    public Student(int id, String name, String course, int sem) {
        this.id = id;
        this.name = name;
        this.course = course;
        this.sem=sem;
    }

    public Student() {

    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCourse() {
        return course;
    }

    public void setCourse(String course) {
        this.course = course;
    }

    public int getSem() {
        return sem;
    }

    public void setSem(int sem) {
        this.sem = sem;
    }
}
